using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace GenericsExc
{
    public class Box<T>
    {
        public Box(T Element)
        {
            element = Element;
        }

        public T element { get;  }

        public override string ToString()
        {
            return $"{typeof(T)}: {element}";
        }
    }
}
